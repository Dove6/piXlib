OBJECT=TESTANN1
TESTANN1:TYPE=ANIMO
TESTANN1:FILENAME=TEST_1.ANN
TESTANN1:VISIBLE=TRUE
TESTANN1:TOCANVAS=TRUE
TESTANN1:ONINIT={THIS^PLAY("MAIN");}
TESTANN1:ONFINISHED^MAIN={THIS^SETPOSITION(1,1);THIS^PLAY("SIDE");}
TESTANN1:ONFINISHED^SIDE={THIS^SETPOSITION(0,0);THIS^PLAY("MAIN");}

